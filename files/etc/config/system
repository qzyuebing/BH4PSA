
config system
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option compat_version '1.1'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option hostname 'BH4PSA'
	option description '4G路由器'

config timeserver 'ntp'
	list server 'ntp.tencent.com'
	list server 'ntp1.aliyun.com'
	list server 'ntp.ntsc.ac.cn'
	list server 'cn.ntp.org.cn'

config led
	option name 'wa'
	option sysfs 'blue:status'
	option trigger 'netdev'
	option dev 'eth1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'wed'
	option sysfs 'red:status'
	option trigger 'netdev'
	option dev 'eth1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option name 'wan'
	option trigger 'netdev'
	option dev 'eth1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'
	option sysfs 'blue:status'

config led
	option name '3.1'
	option trigger 'netdev'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'
	option dev 'br-lan'
	option sysfs 'red:status'

config led
	option name '3.1'
	option sysfs 'blue:status'
	option trigger 'netdev'
	option dev 'lan1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

