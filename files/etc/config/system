
config system
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option compat_version '1.1'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'
	option log_proto 'udp'
	option conloglevel '8'
	option cronloglevel '5'
	option hostname 'IPTV'
	option description '山东有线IPTV'

config timeserver 'ntp'
	list server 'ntp.tencent.com'
	list server 'ntp1.aliyun.com'
	list server 'ntp.ntsc.ac.cn'
	list server 'cn.ntp.org.cn'

config led
	option name 'WAN'
	option sysfs 'green:status'
	option trigger 'netdev'
	option dev 'wan'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led
	option sysfs 'red:status'
	option trigger 'netdev'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'
	option name 'IPTV'
	option dev 'IPTV'

config led
	option name '无线桥接2.4G'
	option sysfs 'blue:status'
	option trigger 'netdev'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'
	option dev 'wlan0'

